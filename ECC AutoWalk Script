$${
// Made By: Cjmk75
// Set time in milla seconds
#time = 78000
IF(autowalk);
 UNSET(autowalk);
  LOG("Autowalk Disabled");
   KEYUP(forward)|WALK();
 STOP();
ELSE;
 SET(autowalk);
  LOG("Autowalk Enabled");
    KEYDOWN(forward);WALK();
  WAIT(%#time%ms)
   KEYUP(forward);
   ECHO(/repair hand);
 WHILE (autowalk);
ENDIF
ENDIF
}$$
