$${
// Made By: Cjmk75
// Set time in milla seconds
#time = 78000
if(autowalk);
 unset(autowalk);
  log("Autowalk Disabled");
   keyup(forward)|WALK();
 stop();
else;
 set(autowalk);
  log("Autowalk Enabled");
    keydown(forward);WALK();
  wait(%#time%ms);
   keyup(forward);
   echo(/repair hand);
 while (autowalk);
endif;
endif;
}$$
